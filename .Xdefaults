*background   : #101010
*foreground   : #fffff2
*font         : -xos4-terminus-*-*-*-*-12-*-*-*-*-*-iso8859-*
*boldFont     : -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-iso8859-*
*transparent  : true
*scrollBar    : false
*secondaryScroll:    true
*pointerBlank : true
*urgentOnBell : true
*modifier	   : alt
*shading      : 30
*fading       : 50
*saveLines    : 10000
*cursorColor  : #DCDCCC
*cursorBlink  : false
*dynamicColors: on
 
*perl-ext-common : default,matcher
*perl-lib        : /usr/lib/*perl/
*urlLauncher     : /usr/bin/firefox
*matcher.button  : 1 

