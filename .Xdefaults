!*font            : -xos4-terminus-*-*-*-*-12-*-*-*-*-*-iso8859-*
!*boldFont        : -xos4-terminus-*-*-*-*-12-*-*-*-*-*-iso8859-*
*font            : -*-profont-*-*-*-*-11-*-*-*-*-*-iso8859-*
*boldFont        : -*-profont-*-*-*-*-11-*-*-*-*-*-iso8859-*
*title           : terminal
*buffered        : true
*transparent     : true
*scrollBar       : false
*secondaryScroll : true
*pointerBlank    : true
*urgentOnBell    : true
*modifier        : alt
*shading         : 70
*fading          : 15
*saveLines       : 10000
*cursorBlink     : false
*dynamicColors   : on
*jumpScroll      : true

*perl-ext-common : default,matcher
*perl-lib        : /usr/lib/*perl/
*urlLauncher     : /usr/bin/firefox
*matcher.button  : 1

*cursorColor     : #80724e
*cursorColor2    : #fff
!*cursorUnderline : true
!*underlineColor  : #66aabb

