*font            : -*-montecarlo-medium-r-normal-*-11-*-*-*-*-*-*-*
*boldFont        : -*-montecarlo-medium-r-normal-*-11-*-*-*-*-*-*-*
*title           : terminal
*buffered        : true
*geometry        : 200x45
*transparent     : true
*scrollBar       : false
*secondaryScroll : true
*pointerBlank    : true
*urgentOnBell    : true
*modifier        : alt
*shading         : 40
*saveLines       : 10000
*cursorBlink     : false
*dynamicColors   : on
*jumpScroll      : true

*perl-ext-common : default,matcher
*perl-lib        : /usr/lib/*perl/
*urlLauncher     : /usr/bin/firefox
*matcher.button  : 1

*cursorColor     : #80724e
*cursorColor2    : #fff
*colorUL         : khaki

! colorscheme
*foreground      :#ddeedd
*background      :#000000
*color0          :#1c1c1c
*color8          :#4D4D4D
*color1          :#cd5c5c
*color9          :#a52a2a
*color2          :#b7ce42
*color10         :#bde077
*color3          :#fea63c
*color11         :#ffe863
*color4          :#66aabb
*color12         :#aaccbb
*color5          :#b7416e
*color13         :#bb4466
*color6          :#5e7175
*color14         :#a3babf
*color7          :#6c887a
*color15         :#ddeedd
